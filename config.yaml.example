# This is the default config file. Do not edit this file. Instead, edit the config.jsonc file in the same directory as this file.
# This is meant to be a reference file for the config.jsonc file. If you edit this file, your changes will be overwritten when the plugin updates.
# If you want to reset your config, delete the config.jsonc file and restart the bot.
#############################################################################################################################################################
Begin of config.yaml:
# This is the config file.
# Do not remove any of the values, as they are required for the bot to function.
# Modify the values to your liking, but do not change the names of the values.
# If you do not know what you are doing, do not modify this file.

# General settings.
command_prefix: "!"
timers_enabled: true
notifications_enabled: true
commands_enabled: true # Changing this to false will disable all chat commands.

# Map settings.
default_grid_size: 150
default_map_margin: 500

# Listener settings. True = enabled, false = disabled.
event_listener_enabled: true # Changing this to false will disable all map event listeners(chinook, helicopter, cargo and oil).
raid_listener_enabled: true
chinook_listener_enabled: true
patrol_helicopter_listener_enabled: true
cargoship_listener_enabled: true
oilrig_listener_enabled: true